<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="TDRPG Animator" file="Animator" main="Main" version="0.0.0" company="Level Up Labs"/>
	
	<window width="1024" height="768" fps="30" orientation="landscape" fullscreen="false"/>
 	
	<set name="BUILD_DIR" value="export" />
	
	<!--<setenv name="no_console" value="1" />-->
	
	<classpath name="source" />
	<assets path="assets" />
	
	<!--<haxedef name="kong"/>-->
	
	<haxelib name="flixel"/>
	<haxelib name="flixel-ui"/>
	<haxelib name="flixel-addons"/>
	<haxelib name="openfl"/>
	<haxelib name="firetongue"/>
	<haxelib name="task"/>
	<haxelib name="systools"/>

	<haxedef name="FLX_BMP_DEBUG"/>
	<haxedef name="FLX_NO_DEBUG" unless="debug"/>
		
</project>